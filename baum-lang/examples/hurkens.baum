-- τ = λ(t: P (P u)) (λ(X: U, f, p: P X) t λ(x) p (f (x X f))) of u
-- τ = λ(t: P (P u)) (λ(X: U, f: P (P X) → X, p) t λ(x) p (f (x X f))) of u
-- τ = λ(t: P (P u)) (λ(X: U, f: P (P _) → X, p) t λ(x) p (f (x X f))) of u
