syntax 1> A → B = Π(x: A) B
syntax 1> (x: A) → B = Π(x: A) B

id {A: U} (x: A) = x
syntax 0 x of A = id {A} x

⊥ = (A: U) → A
¬ (A: U) = A → ⊥
P (A: U) = A → U
u = (X: U) → (P (P X) → X) → P (P X)

τ = λ(t: P (P u)) (λ(X: U, f: P (P X) → X, p: P X) t λ(x: u) p (f (x X f))) of u
