syntax 0.1> A → B = Π(x: A) B
syntax 0.1> (x: A) → B = Π(x: A) B

two (a: U) (f: a → a) (x: a): a = f (f x)
three (a: U) (f: a → a) (x: a): a = f (f (f x))

module A = {
  nine (a: U): (a → a) → a → a = (two (a → a)) (three a)
  eight (a: U) (f: a → a) (x: a): a = (three (a → a)) (two a) f x
}

n9 = A.nine
n8 = A.eight
